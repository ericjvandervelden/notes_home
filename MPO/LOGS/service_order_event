from SERVICE_ORDER_EVENT serviceord0_
inner join SERVICE_ORDER serviceord1_ on serviceord0_.SERVICE_ORDER_SYSTEMID=serviceord1_.SYSTEM_ID
inner join CONSIGNMENT consignmen2_ on serviceord1_.SYSTEM_ID=consignmen2_.SERVICE_ORDER_SYSTEMID
inner join SERVICE_ACTION serviceact3_ on consignmen2_.SYSTEM_ID=serviceact3_.CONSIGNMENT_SYSTEMID
inner join SHIPMENT_ORDER shipmentor4_ on serviceact3_.SHIPMENT_ORDER_SYSTEMID=shipmentor4_.SYSTEM_ID
inner join ORGANIZATION organizati5_ on shipmentor4_.ORGANIZATION_SYSTEMID=organizati5_.SYSTEM_ID
left outer join SHIPMENT_SERVICE_LEVEL shipmentse13_ on shipmentor4_.ACT_SERVICE_LEVEL_SYSTEMID=shipmentse13_.SYSTEM_ID
left outer join SERVICE_ACTION_TYPE serviceact12_ on serviceact3_.SERVICE_ACTION_TYPE_SYSTEMID=serviceact12_.SYSTEM_ID
left outer join COUNTRY country6_ on serviceord1_.FROM_COUNTRY_SYSTEMID=country6_.SYSTEM_ID
left outer join COUNTRY country7_ on serviceord1_.TO_COUNTRY_SYSTEMID=country7_.SYSTEM_ID
left outer join EVENT_EVENT eventvo8_ on serviceord0_.LAST_CLOSED_SYSTEMID=eventvo8_.SYSTEM_ID
left outer join EVENT_TYPE eventtype9_ on eventvo8_.EVENT_TYPE_SYSTEMID=eventtype9_.SYSTEM_ID
left outer join EVENT_EVENT eventvo10_ on serviceord0_.NEXT_TO_CLOSE_SYSTEMID=eventvo10_.SYSTEM_ID
left outer join EVENT_TYPE eventtype11_ on eventvo10_.EVENT_TYPE_SYSTEMID=eventtype11_.SYSTEM_ID
where (shipmentor4_.ORGANIZATION_SYSTEMID in (
    select userorgani16_.ORGANIZATION_SYSTEMID
    from USER_ORGANIZATION userorgani16_
    , ORGANIZATION organizati17_
    where userorgani16_.ORGANIZATION_SYSTEMID=organizati17_.SYSTEM_ID
    and userorgani16_.USER_SYSTEMID=27223886))
and shipmentor4_.IS_DELETED=0
and (lower(organizati5_.ORGANIZATION_ID) like lower('%N0008574%')
or lower(shipmentor4_.SHIPMENT_ORDER_ID) like lower('%N0008574%')
or lower(consignmen2_.CONSIGNMENT_NUMBER) like lower('%N0008574%')
or lower(serviceact12_.SERVICE_ACTION_TYPE) like lower('%N0008574%')
or lower((consignmen2_.FROM_LOCATION_ID||' - '||consignmen2_.FROM_NAME||' - '||consignmen2_.FROM_CITY||' - '||coalesce(country6_.COUNTRY_CODE, ''))) like lower('%N0008574%')
or lower((consignmen2_.TO_LOCATION_ID||' - '||consignmen2_.TO_NAME||' - '||consignmen2_.TO_CITY||' - '||coalesce(country7_.COUNTRY_CODE, ''))) like lower('%N0008574%')
or lower(shipmentse13_.SERVICE_LEVEL) like lower('%N0008574%')
or lower(eventtype9_.NAME) like lower('%N0008574%')
or lower(eventtype11_.NAME) like lower('%N0008574%'))
;
rollback ;
