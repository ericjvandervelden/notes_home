select
customeror0_.SYSTEM_ID as col_0_0_,
customeror0_.CUSTOMER_ORDER_NUMBER as col_1_0_,
(
    select customeror1_.REQUESTED_END_AFTER
    from CUSTOMER_ORDER_LINE customeror1_,
    CUSTOMER_ORDER customeror2_
    where customeror1_.CUSTOMER_ORDER_SYSTEMID=customeror2_.SYSTEM_ID
    and customeror1_.CUSTOMER_ORDER_SYSTEMID=customeror0_.SYSTEM_ID
    and customeror1_.SYSTEM_ID=(
        select max(customeror3_.SYSTEM_ID)
        from CUSTOMER_ORDER_LINE customeror3_
        where customeror3_.CUSTOMER_ORDER_SYSTEMID=customeror0_.SYSTEM_ID
    )
) as col_2_0_,
customerst5_.CODE as col_3_0_
from CUSTOMER_ORDER customeror0_,
CUSTOMER_STATUS customerst5_
where customeror0_.CUSTOMER_STATUS_SYSTEMID=customerst5_.SYSTEM_ID
and customeror0_.SELLER_PARTY_ID=80387250                           -- party_id van de vendor
and customerst5_.CODE='Acknowledged' limit 1024;
rollback ;
