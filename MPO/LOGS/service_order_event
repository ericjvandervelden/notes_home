select
customeror0_.SYSTEM_ID as col_0_0_,
customeror0_.CUSTOMER_ORDER_NUMBER as col_1_0_, (
    select (customeror2_.TO_CITY||', '||country3_.COUNTRY_NAME)
    from CUSTOMER_ORDER_LINE customeror2_,
    COUNTRY country3_,
    CUSTOMER_ORDER customeror4_
    where customeror2_.TO_COUNTRY_SYSTEMID=country3_.SYSTEM_ID
    and customeror2_.CUSTOMER_ORDER_SYSTEMID=customeror4_.SYSTEM_ID
    and customeror2_.CUSTOMER_ORDER_SYSTEMID=customeror0_.SYSTEM_ID
    and customeror2_.SYSTEM_ID=(
        select max(customeror5_.SYSTEM_ID)
        from CUSTOMER_ORDER_LINE customeror5_
        where customeror5_.CUSTOMER_ORDER_SYSTEMID=customeror0_.SYSTEM_ID
    )
) as col_2_0_, (
    select customeror7_.REQUESTED_END_AFTER
    from CUSTOMER_ORDER_LINE customeror7_,
    CUSTOMER_ORDER customeror8_
    where customeror7_.CUSTOMER_ORDER_SYSTEMID=customeror8_.SYSTEM_ID
    and customeror7_.CUSTOMER_ORDER_SYSTEMID=customeror0_.SYSTEM_ID
    and customeror7_.SYSTEM_ID=(
        select max(customeror9_.SYSTEM_ID)
        from CUSTOMER_ORDER_LINE customeror9_
        where customeror9_.CUSTOMER_ORDER_SYSTEMID=customeror0_.SYSTEM_ID
    )
) as col_3_0_,
customerst11_.CODE as col_4_0_
from CUSTOMER_ORDER customeror0_,
CUSTOMER_STATUS customerst11_
cross join OMS_USER_PARTY userparty1_
cross join PARTY partyvo12_
where customeror0_.CUSTOMER_STATUS_SYSTEMID=customerst11_.SYSTEM_ID
and userparty1_.PARTY_SYSTEMID=partyvo12_.SYSTEM_ID
and customeror0_.SELLER_PARTY_ID=partyvo12_.PARTY_ID
and userparty1_.USER_SYSTEMID=27223886
and customerst11_.CODE='ControlCompleted' limit 1000;
